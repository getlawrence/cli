{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Analyze C#",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["analyze", "${workspaceFolder}/examples/csharp"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "Analyze JS",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["analyze", "${workspaceFolder}/examples/js"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "Analyze Python",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["analyze", "${workspaceFolder}/examples/python"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "gen (dry-run)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["gen", "${workspaceFolder}/examples", "--dry-run"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "gen (live)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["gen", "${workspaceFolder}/examples-1"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "gen specific project",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["gen", "${workspaceFolder}/examples/${input:projectName}", "--dry-run"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb status",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "status"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb update all",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "update"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb update go",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "update", "go", "--force"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb update js",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "update", "js"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb update python",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "update", "python"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb update java",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "update", "java"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb update csharp",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "update", "csharp"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb update php",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "update", "php"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb update ruby",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "update", "ruby"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb update with custom output",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "update", "--output", "custom_kb.db"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "kb update force",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["knowledge", "update", "--force"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "registry sync",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["registry", "sync"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "registry sync force",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["registry", "sync", "--force"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "registry sync custom path",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["registry", "sync", "--path", "custom_registry"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "registry info",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["registry", "info"],
            "env": {"GO111MODULE": "on"}
        },
        {
            "name": "registry info custom path",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["registry", "info", "--registry", "custom_registry"],
            "env": {"GO111MODULE": "on"}
        }
    ],
    "inputs": [
        {
            "id": "projectName",
            "description": "Enter project name (js, python, csharp, etc.)",
            "default": "js",
            "type": "promptString"
        }
    ]
}