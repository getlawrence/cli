{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Launch analyze",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [
                "analyze",
                "${workspaceFolder}/examples/monorepo-sample",
            ],
            "env": {
                "GO111MODULE": "on"
            }
        },
        {
            "name": "Launch codegen",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [
                "codegen",
                "--path",
                "${workspaceFolder}/examples/go-sample",
            ],
            "env": {
                "GO111MODULE": "on"
            }
        }
    ]
}